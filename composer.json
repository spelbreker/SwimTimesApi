{
    "name": "spelbreker/SwimTimesApi",
    "type": "library",
    "description": "swim times api connector",
    "keywords": [
        "patrickBrouwer",
        "SwimTimesApi"
    ],
    "homepage": "https://github.com/patrickBrouwer/SwimTimesApi",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Brouwer",
            "email": "info@patrickdevelopment.nl",
            "homepage": "http://patrickdevelopment.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PatrickBrouwer\\SwimTimesApi\\SwimTimesApiServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "PatrickBrouwer\\SwimTimesApi\\": "src/swimTimes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PatrickBrouwer\\SwimTimesApi\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
